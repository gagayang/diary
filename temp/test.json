  "husky": {
    "hooks": {
      "commit-msg": "commitlint .commitlintrc.js -E HUSKY_GIT_PARAMS",
      "pre-push": "cd ../../../ && DEBUG=eslint:cli-engine npx eslint --fix --cache --quiet $(git diff --diff-filter=ACMR --name-only $(git merge-base $(git rev-parse --abbrev-ref HEAD) $([[ $(git branch -a | grep -w remotes/origin/$(git rev-parse --abbrev-ref HEAD)) ]] && echo remotes/origin/$(git rev-parse --abbrev-ref HEAD) || echo master)) | grep -E '.(ts|tsx)$' | xargs)"
    }
  }
